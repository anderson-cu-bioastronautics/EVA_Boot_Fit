journal = {Clinical Biomechanics},
keywords = {Adult,Aged,Aged- 80 and over,Body Mass Index,Body Weight,Body mass index,Cross-Sectional Studies,Folder - Foot Models,Foot,Humans,Male,Middle Aged,Morphology,Obesity,Overweight,Young Adult},
