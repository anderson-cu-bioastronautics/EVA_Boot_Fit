keywords = {Anatomical reference frames,Folder - Multi-segment Foot Model (Modelling Human,Fore-foot,Gait analysis,Kinematics,Metatarsal bones,Metatarso-phalangeal joint,Metatarsus,Mid-foot,Rear-foot},
mendeley-groups = {Foot Structure/Biomechanics},
mendeley-tags = {Anatomical reference frames,Folder - Multi-segment Foot Model (Modelling Human,Fore-foot,Gait analysis,Kinematics,Metatarsal bones,Metatarso-phalangeal joint,Metatarsus,Mid-foot,Rear-foot},
