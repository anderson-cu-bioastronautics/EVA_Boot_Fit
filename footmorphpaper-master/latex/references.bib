doi = {10.15221/16.201},
file = {:C$\backslash$:/Users/abhis/Dropbox/References/Files/Kim et al. - 2016 - A Parametric Model of Shoulder Articulation for Virtual Assessment of Space Suit Fit.pdf:pdf},
keywords = {Folder - General Injury Research,anthropometry,computer aided design,cost reduction,deformation,human factors engineering,injuries,scanners,shoulders,space suits,statistical analysis},
mendeley-groups = {Spacesuits/Fit},
mendeley-tags = {Folder - General Injury Research,anthropometry,computer aided design,cost reduction,deformation,human factors engineering,injuries,scanners,shoulders,space suits,statistical analysis},
